                </value>
            </inputParameters>
            <inputParameters>
                <name>middleValue</name>
                <value>
                    <elementReference>RewardChangeDetails_value</elementReference>
                </value>
            </inputParameters>
            <isRequired>true</isRequired>
            <storeOutputAutomatically>true</storeOutputAutomatically>
        </fields>
        <fields>
            <name>Amount</name>
            <dataType>Number</dataType>
            <fieldText>$ Amount</fieldText>
            <fieldType>InputField</fieldType>
            <isRequired>false</isRequired>
            <scale>0</scale>
            <visibilityRule>
                <conditionLogic>1 AND 2</conditionLogic>
                <conditions>
                    <leftValueReference>RewardChange_dependent.topValue</leftValueReference>
                    <operator>EqualTo</operator>
                    <rightValue>
                        <stringValue>AccommodationPoints</stringValue>
                    </rightValue>
                </conditions>
                <conditions>
                    <leftValueReference>RewardChange_dependent.middleValue</leftValueReference>
                    <operator>IsNull</operator>
                    <rightValue>
                        <booleanValue>false</booleanValue>
                    </rightValue>
                </conditions>
            </visibilityRule>
        </fields>
        <fields>
            <name>Reward_Number</name>
            <dataType>String</dataType>
            <fieldText>Reward Number</fieldText>
            <fieldType>InputField</fieldType>
            <isRequired>false</isRequired>
            <validationRule>
                <errorMessage>&lt;p&gt;The Reward Number you entered is not valid. Please try again.&lt;/p&gt;</errorMessage>
                <formulaExpression>{!RewardNumber_form} = {!Get_matching_Reward_records.SequenceNumber__c}</formulaExpression>
            </validationRule>
            <visibilityRule>
                <conditionLogic>(1 OR 2 or 4) AND 3</conditionLogic>
                <conditions>
                    <leftValueReference>RewardChange_dependent.topValue</leftValueReference>
                    <operator>EqualTo</operator>
                    <rightValue>
                        <elementReference>Expire</elementReference>
                    </rightValue>
                </conditions>
                <conditions>
                    <leftValueReference>RewardChange_dependent.topValue</leftValueReference>
                    <operator>EqualTo</operator>
                    <rightValue>
                        <elementReference>Redeem</elementReference>
                    </rightValue>
                </conditions>
                <conditions>
                    <leftValueReference>RewardChange_dependent.middleValue</leftValueReference>
                    <operator>IsNull</operator>
                    <rightValue>
                        <booleanValue>false</booleanValue>
                    </rightValue>
                </conditions>
                <conditions>
                    <leftValueReference>RewardChange_dependent.topValue</leftValueReference>
                    <operator>EqualTo</operator>
                    <rightValue>
                        <stringValue>Extend</stringValue>
                    </rightValue>
                </conditions>
            </visibilityRule>
        </fields>
        <fields>
            <name>Comments</name>
            <fieldText>Comments</fieldText>
            <fieldType>LargeTextArea</fieldType>
            <isRequired>false</isRequired>
            <validationRule>
                <errorMessage>&lt;p&gt;&lt;span style=&quot;background-color: rgb(255, 255, 255); color: rgb(62, 62, 60);&quot;&gt;The Comments contains more than 255 characters. Please shorten and try again.&lt;/span&gt;&lt;/p&gt;</errorMessage>
                <formulaExpression>LEN({!Comments}) &lt; 255</formulaExpression>
            </validationRule>
