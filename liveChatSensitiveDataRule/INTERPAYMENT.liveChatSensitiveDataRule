<?xml version="1.0" encoding="UTF-8"?>
<LiveChatSensitiveDataRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <actionType>Replace</actionType>
    <description>Interpayment Credit Card details masking</description>
    <enforceOn>7</enforceOn>
    <isEnabled>true</isEnabled>
    <pattern>^636\d{1}[\s\-]*\d{4,5}[\s\-]*\d{4,5}[\s\-]*\d{4,5}$
\b636\d{1}[\s\-]*\d{4,5}[\s\-]*\d{4,5}[\s\-]*\d{4,5}\b
^636\d{1}[\s\-]*\d{4}[\s\-]*\d{4}[\s\-]*\d{4}$
\b636\d{1}[\s\-]*\d{4}[\s\-]*\d{4}[\s\-]*\d{4}\b
^636\{16,19}$
\b636\d{16,19}\b</pattern>
    <priority>8</priority>
    <replacement>***************</replacement>
</LiveChatSensitiveDataRule>
